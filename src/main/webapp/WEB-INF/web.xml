<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
	version="4.0">
	<display-name>kilatopont</display-name>
	<servlet>
		<description>Kilatopont servlet</description>
		<servlet-name>KilatoServlet</servlet-name>
		<servlet-class>hu.godenyd.servlet.kilatopont.KilatoServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>KilatoServlet</servlet-name>
		<url-pattern>/kilato</url-pattern>
	</servlet-mapping>
	<filter>
		<filter-name>CharacterSetFilter</filter-name>
		<filter-class>hu.godenyd.servlet.kilatopont.filter.CharacterSetFilter</filter-class>
	</filter>
	<filter-mapping>
		<filter-name>CharacterSetFilter</filter-name>
		<url-pattern>/*</url-pattern>
	</filter-mapping>
</web-app>